create table student( a_num INT, last TEXT, first TEXT  );
insert into student values(1234,"Smith","Jane");
insert into student values(2345,"Lincoln","Abraham");
insert into student values(3456,"Franklin","Benjamin");
insert into student values(4567,"Doe","John");

create table schedule( a_num INT, crn INT );
insert into schedule value(1234,0000);
